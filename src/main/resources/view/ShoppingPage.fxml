<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="591.0" prefWidth="857.0"
            style="-fx-background-color: white;" stylesheets="@../css/product.css"
            xmlns="http://javafx.com/javafx/11.0.1">
   <children>
       <TableView fx:id="shoppingListTable" prefHeight="591.0" prefWidth="857.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="35.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
