      </TableView>
       <Label fx:id="totalCostLabel" layoutX="186.0" layoutY="16.0" text="\$0.00" AnchorPane.topAnchor="5.0">
         <font>
            <Font name="Calibri Bold Italic" size="24.0" />
         </font>
      </Label>
       <Label layoutX="14.0" layoutY="16.0" text="Total Cost ($):" AnchorPane.leftAnchor="15.0"
              AnchorPane.topAnchor="5.0">
         <font>
            <Font name="Calibri Bold Italic" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
