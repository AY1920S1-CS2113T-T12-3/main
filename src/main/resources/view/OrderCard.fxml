<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<fx:root maxHeight="1.7976931348623157E308" minHeight="230.0" prefHeight="200.0" prefWidth="800.0"
         style="-fx-background-color: white;" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane fx:id="innerPane" prefHeight="200.0" prefWidth="200.0"
                    style="-fx-background-color: white; -fx-border-color: #CCCCCC; -fx-effect: dropshadow(three-pass-box, grey, 10.0,0, 0, 0);"
                    AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"
                    AnchorPane.topAnchor="15.0">
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0" spacing="10.0"
                              style="-fx-background-color: #5086ec;">
                            <children>
                                <Label fx:id="index" text="#14g4ar" textFill="WHITE">
                                    <font>
                                        <Font name="Arial" size="18.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="deadline" alignment="CENTER"
                                       style="-fx-background-color: white; -fx-background-radius: 5; -fx-padding: 4;"
                                       text="March 18" textAlignment="CENTER" textFill="#5086ec">
                                    <font>
                                        <Font name="Arial" size="18.0"/>
                                    </font>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <graphic>
                                        <ImageView fitHeight="17.0" fitWidth="17.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../images/alarm-clock.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Label>
                                <Label fx:id="status" alignment="CENTER"
                                       style="-fx-background-color: rgb(101,203,87); -fx-background-radius: 5; -fx-padding: 4;"
                                       text="active" textAlignment="CENTER" textFill="WHITE">
                                    <font>
                                        <Font name="Arial" size="18.0"/>
                                    </font>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                </Label>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                            <children>
                                <Label alignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0"/>
                                    </HBox.margin>
                                    <graphic>
                                        <ImageView fitHeight="150.0" fitWidth="15.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../images/name.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Label>
                                <Label fx:id="name" alignment="CENTER" text="Jiajun">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0" right="20.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label alignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="20.0"/>
                                    </HBox.margin>
                                    <graphic>
                                        <ImageView fitHeight="150.0" fitWidth="15.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../images/phone-contact.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Label>
                                <Label fx:id="contact" alignment="CENTER" text="83486144">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0" right="20.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label alignment="CENTER">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="20.0"/>
                                    </HBox.margin>
                                    <graphic>
                                        <ImageView fitHeight="150.0" fitWidth="15.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../images/commentary.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Label>
                                <Label fx:id="remarks" alignment="CENTER" text="No nuts">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0"/>
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0"/>
                        <HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                            <children>
                                <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0"
                                            style="-fx-background-color: white;" HBox.hgrow="ALWAYS">
                                    <content>
                                        <FlowPane fx:id="itemFlow" alignment="CENTER_LEFT" columnHalignment="CENTER"
                                                  hgap="10.0" prefHeight="200.0" prefWidth="200.0"
                                                  style="-fx-background-color: white;" vgap="10.0">
                                            <padding>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                                            </padding>
                                        </FlowPane>
                                    </content>
                                </ScrollPane>
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0"/>
                        <HBox alignment="TOP_RIGHT" prefHeight="18.0" prefWidth="200.0">
                            <children>
                                <Label alignment="CENTER" text="Order ID:">
                                    <font>
                                        <Font name="Arial" size="12.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="id" alignment="CENTER" text="Jiajun" textFill="#96663b">
                                    <font>
                                        <Font name="Arial" size="12.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0" right="20.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label alignment="CENTER" text="Created on:">
                                    <font>
                                        <Font name="Arial" size="12.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="20.0"/>
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="creationDate" alignment="CENTER" text="01/01/1999 18:00"
                                       textFill="#96663b">
                                    <font>
                                        <Font name="Arial" size="12.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0" right="20.0"/>
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </children>
</fx:root>
